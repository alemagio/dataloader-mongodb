{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**"]
    },
    "publish": {
      "outputs": [],
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {
      "outputs": [],
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "outputs": [],
      "dependsOn": [
        "^build"
      ],
      "cache": false
    },
    "test:ci": {
      "outputs": [],
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    },
    "unit": {
      "outputs": [],
      "dependsOn": [
        "^build"
      ],
      "cache": false
    }
  }
}
